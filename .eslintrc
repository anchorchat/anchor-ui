{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/no-noninteractive-element-interactions": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "jsx-a11y/mouse-events-have-key-events": 0,
    "react/no-danger": 0,
    "jsx-a11y/label-has-for": [ 2,
      { "components": [ "Label" ],
        "required":
        { "every": [ "id" ] },
      "allowChildren": false }
    ]
  },
  "globals": {
    "Image": true,
    "window": true
  },
  "plugins": [
    "mocha"
  ]
}
